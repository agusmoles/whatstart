{
  "name": "whatstart",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:ci": "jest --runInBand",
    "lint:check": "eslint src/**/*.ts src/**/*.tsx --max-warnings 5",
    "lint:fix": "eslint src/**/*.ts src/**/*.tsx --fix",
    "format:check": "prettier --check \"src/**/*.(ts|tsx|js|json|yaml)\"",
    "format:write": "prettier --write \"src/**/*.(ts|tsx|js|json|yaml)\""
  },
  "dependencies": {
    "next": "12.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/node": "18.7.1",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "eslint": "8.21.0",
    "eslint-config-next": "12.2.4",
    "prettier": "2.7.1",
    "typescript": "4.7.4"
  }
}
